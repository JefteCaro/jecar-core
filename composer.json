{
    "name": "jecar/core",
    "type": "library",
    "autoload": {
        "files": [
            "vendor/laravel/framework/src/Illuminate/Collections/helpers.php",
            "vendor/laravel/framework/src/Illuminate/Events/functions.php",
            "vendor/laravel/framework/src/Illuminate/Foundation/helpers.php",
            "vendor/laravel/framework/src/Illuminate/Support/helpers.php"
        ],
        "psr-4": {
            "Jecar\\Core\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jecar",
            "email": "carojefte@gmail.com"
        }
    ],
    "require": {
        "ankitpokhrel/tus-php": "*",
        "league/glide": "*",
        "league/glide-laravel": "dev-master",
        "illuminate/support": "8.x-dev"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Jecar\\Core\\Providers\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "laravel/framework": "8.x-dev"
    }
}
